@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model WebApplication1.Models.IndexViewModel
@{
    ViewBag.Title = "Управление";
}
@{
    var beg = ViewBag.UInfo;


    <h2>@beg.UserName</h2>

    <div class="row">
        <div class="col-sm-3">
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTxHGpR68174qJmNG1yZvsYxY62PneWPB-E5X1uOH2bRzV16qhs_w" style="width: 100%; height: 100%">
        </div>
        <div class="col-sm-9" style="padding-left: 0px">
            <div class="jumbotron jumbotron_developers">
                <h1 class="text-center">ASP.NET</h1>
                <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
            </div>
        </div>
    </div>
    <a type="button" class="btn btn-primary" style="border-top: 30px; border-bottom: 30px; border-left:50px" href="@Url.Action("CreatureOrder", "Manage")">Primary</a>
    <div class="tabbable">
        <!-- Only required for left/right tabs -->
        <ul class="nav nav-tabs">
            <li class="active">
                <a href="#tab1" data-toggle="tab">Вкладка 1</a>
            </li>
            <li>
                <a href="#tab2" data-toggle="tab">Вкладка 2</a>
            </li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane active" id="tab1">
                <p>Я первая секция.</p>
            </div>
            <div class="tab-pane" id="tab2">
                <p>Привет, я 2-я секция.</p>
            </div>
        </div>
    </div>
}

