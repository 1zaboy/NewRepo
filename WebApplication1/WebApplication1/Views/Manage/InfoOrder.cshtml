@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <h1>Name</h1>
    <div>
        @ViewBag.Info2.MainName;
    </div>
    <h1>Price</h1>
    <div>
        @ViewBag.Info2.Price;
    </div>
    <h1>DateIn</h1>
    <div>
        @ViewBag.Info2.DateIn;
    </div>
    <h1>DateOut</h1>
    <div>
        @ViewBag.Info2.DateOut;
    </div>
    <h1>File</h1>
    <div>
        @Html.Raw("<img class=\"card_user_style1\" src=\"data:image/jpeg;base64,"
                                                           + Convert.ToBase64String(ViewBag.Info2.WayFile) + "\" />");
    </div>
    <h1>Discription</h1>
    <div>
        @ViewBag.Info2.Discription;
    </div>
</div>


<a type="button" class="btn btn-primary" style="margin: 10px; margin-left: 60px" href="@Url.Action("UserInfo", "Manage", new { UserId = ViewBag.Info1.Id})">Customer</a>