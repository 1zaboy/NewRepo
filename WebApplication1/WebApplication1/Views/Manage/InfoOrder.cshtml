}
<div class="row">
    <h1>Name</h1>
    <div>
        @ViewBag.Info2.MainName;
    </div>
    <h1>Price</h1>
    <div>
        @ViewBag.Info2.Price
    </div>
    <h1>DateIn</h1>
    <div>
        @ViewBag.Info2.DateIn
    </div>
    <h1>DateOut</h1>
