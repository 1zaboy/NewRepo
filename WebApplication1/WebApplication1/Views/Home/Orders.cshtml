@using WebApplication1;
@using WebApplication1.Controllers;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row row_position">
    @foreach (DbUserOrder ss in ViewBag.Ord)
    {
        <dov class="_col-lg-4">
            <div class="card text-center" style="width: 70%; margin: auto">
                @if (ss.WayFile != null)
    {
            @Html.Raw("<img class=\"card_user_style1\" src=\"data:image/jpeg;base64,"
                       + Convert.ToBase64String(@ss.WayFile) + "\" />")
}
else
{
            <img class="card-img-top card_user_style1" src="~/Content/image/icon_about_us.png" alt="Card image cap">
}
                <div class="card-block">
                    <h4 class="card-title text-center">@ss.MainName</h4>
                    <p class="card-text text_left">@ss.Price</p>
                    <p class="card-text text_left">@ss.Discription</p>
                    <a href="#" class="btn btn-primary">Go somewhere</a>
                </div>
            </div>
        </dov>
    }
</div>
