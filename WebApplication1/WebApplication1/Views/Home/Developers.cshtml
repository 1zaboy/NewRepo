@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron jumbotron_developers">
    <h1 class="text-center">Developer Board</h1>
    <p class="lead" >Крупное сообщество, которое сочитает в себе два крупных кластера: разрабочики и заказчики.</p>
</div>
<div class="container">
    <table class="table table-hover">
        <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone Number</th>
        </tr>
        </thead>
        @foreach(AspNetUsers ss in ViewBag.UserListOnPrint)
        { 
        <tbody>
        <tr onclick="window.location='@Url.Action("UserInfo", "Manage", new { UserId = @ss.Id})#';">
            <td>@ss.UserName</td>
            <td>@ss.Email</td>
            <td>
                @ss.Id
                
            </td>
        </tr>
        </tbody>
        }
        
    </table>
</div>


